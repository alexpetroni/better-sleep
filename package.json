{
	"name": "sveltekit-shadcn-blog-starter",
	"version": "1.0.0",
	"description": "A modern blog starter template built with SvelteKit, shadcn-svelte, Tailwind CSS, and MDsveX",
	"author": "Yusuf Cengiz",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/YusufCeng1z/sveltekit-shadcn-blog-starter"
	},
	"keywords": [
		"sveltekit",
		"svelte",
		"blog",
		"starter",
		"template",
		"shadcn",
		"tailwindcss",
		"mdsvex",
		"markdown",
		"typescript"
	],
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"mdsvex": "^0.12.6",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"mode-watcher": "^1.1.0"
	}
}